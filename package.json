{
  "name": "react-vite-mdx-tailwind",
  "version": "0.0.1",
  "description": "An example application using React 18, Vite, MDX 2, and Tailwind CSS v3",
  "main": "index.tsx",
  "author": "Tom Busillo",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@2.3.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pretty": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky install",
    "lint-staged": "yarn pretty && yarn lint",
    "pre-commit": "yarn format && lint-staged --config lint-staged.json",
    "generate": "plop",
    "test:unit": "vite-jest --no-cache",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/rollup": "^2.1.2",
    "@monaco-editor/react": "^4.4.6",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^27.5.0",
    "@types/mdx": "^2.0.2",
    "@types/mdx-js__react": "^1.5.5",
    "@types/mocha": "^10.0.1",
    "@types/prismjs": "^1.26.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-prismjs": "^2.1.0",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.3",
    "happy-dom": "^6.0.4",
    "history": "^5.3.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "mocha": "^10.1.0",
    "monaco-editor": "^0.34.1",
    "plop": "^3.1.1",
    "postcss": "^8.4.16",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "refractor": "^4.8.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-shiki": "^0.0.9",
    "rehype-slug": "^5.0.1",
    "rehype-slug-custom-id": "^1.1.0",
    "remark-code-frontmatter": "^1.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-github": "^11.2.4",
    "remark-images": "^3.1.0",
    "remark-mdx": "^2.1.3",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
    "rollup": "^2.78.1",
    "tailwindcss": "^3.2.4",
    "unified": "^10.1.2",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.29.8",
    "babel-jest": "^28.1.3",
    "c8": "^7.11.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-vitest": "^0.0.8",
    "husky": "^8.0.1",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.9",
    "vite-plugin-eslint": "^1.7.0",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.25.0"
  }
}
